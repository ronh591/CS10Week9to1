#include <iostream>
#include <string>
using namespace std;
int main() {
string name_of_string = "hello";
for (int i = 0; i < name_of_string.length(); i++)
{
    cout << name_of_string[i] << " ";
    cout << name_of_string.at(i) << " ";
    
    
}
for(int i = name_of_string.length() - 1; i>=0; i--)
{
   cout << name_of_string[i] << " "; 
   cout << name_of_string.at(i) << " ";
} 
}
