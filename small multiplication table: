#include <iostream>
using namespace std;

int main() {
int row = 0;
int col = 0;// I initialized the variables in main() before both nested loops
for (row=1; row<=3; row++)  //outer​
{
   for (col=1; col<=3; col++)//inner​
   {
        cout << row * col << endl;
    }
 }
}
/* row 1 * col 1= 1, 1*2=2, 1*3=3 row 2*col1= 2 2*2=4, 2*3=6, row 3*col1=1, 3*2=6, 3*3=9{123246369}
output = 1
2
3
2
4
6
3
6
9*/
